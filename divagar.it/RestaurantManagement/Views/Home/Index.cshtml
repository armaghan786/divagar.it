<script>
    document.addEventListener('DOMContentLoaded', function () {
        // Select necessary DOM elements
        const carousel = document.querySelector('.carousel');
        const slidesContainer = carousel.querySelector('.carousel__slides');
        const slides = carousel.querySelectorAll('.carousel__slide');
        const prevArrow = carousel.querySelector('.carousel__arrow svg:first-child');
        const nextArrow = carousel.querySelector('.carousel__arrow svg:last-child');
        const paginationContainer = carousel.querySelector('.carousel__pagination-container');
        const paginationDots = paginationContainer.querySelectorAll('.carousel__pagination');

        let currentIndex = 0;
        const totalSlides = slides.length;
        const slideInterval = 5000; // Auto-slide interval in milliseconds
        let autoSlideInterval;

        // Initialize carousel
        function initCarousel() {
            // Clone first and last slides for infinite looping effect
            const firstSlide = slides[0].cloneNode(true);
            const lastSlide = slides[totalSlides - 1].cloneNode(true);

            slidesContainer.appendChild(firstSlide);
            slidesContainer.insertBefore(lastSlide, slidesContainer.firstChild);

            // Update slides collection after cloning
            const allSlides = carousel.querySelectorAll('.carousel__slide');

            // Set initial position
            slidesContainer.style.transform = `translateX(-100%)`;

            // Set transition effect
            slidesContainer.style.transition = 'transform 0.5s ease-in-out';

            // Start auto-slide
            startAutoSlide();

            // Add event listeners
            nextArrow.addEventListener('click', moveToNextSlide);
            prevArrow.addEventListener('click', moveToPrevSlide);

            // Handle transition end for infinite looping
            slidesContainer.addEventListener('transitionend', handleTransitionEnd);
        }

        // Move to next slide
        function moveToNextSlide() {
            if (currentIndex >= totalSlides) return;
            currentIndex++;
            updateSlidePosition();
            resetAutoSlide();
        }

        // Move to previous slide
        function moveToPrevSlide() {
            if (currentIndex <= -1) return;
            currentIndex--;
            updateSlidePosition();
            resetAutoSlide();
        }

        // Update slide position based on currentIndex
        function updateSlidePosition() {
            slidesContainer.style.transform = `translateX(-${(currentIndex + 1) * 100}%)`;
            updatePagination();
        }

        // Update pagination dots
        function updatePagination() {
            paginationDots.forEach((dot, index) => {
                dot.classList.toggle('carousel__pagination-active', index === currentIndex);
                dot.classList.toggle('carousel__pagination-inactive', index !== currentIndex);
            });
        }

        // Handle infinite looping effect
        function handleTransitionEnd() {
            const allSlides = carousel.querySelectorAll('.carousel__slide');
            if (currentIndex === totalSlides) {
                slidesContainer.style.transition = 'none';
                currentIndex = 0;
                slidesContainer.style.transform = `translateX(-${(currentIndex + 1) * 100}%)`;
                requestAnimationFrame(() => {
                    slidesContainer.style.transition = 'transform 0.5s ease-in-out';
                });
            }
            if (currentIndex === -1) {
                slidesContainer.style.transition = 'none';
                currentIndex = totalSlides - 1;
                slidesContainer.style.transform = `translateX(-${(currentIndex + 1) * 100}%)`;
                requestAnimationFrame(() => {
                    slidesContainer.style.transition = 'transform 0.5s ease-in-out';
                });
            }
        }

        // Start auto-slide
        function startAutoSlide() {
            autoSlideInterval = setInterval(moveToNextSlide, slideInterval);
        }

        // Reset auto-slide timer
        function resetAutoSlide() {
            clearInterval(autoSlideInterval);
            startAutoSlide();
        }

        // Initialize the carousel
        initCarousel();
    });

</script>
